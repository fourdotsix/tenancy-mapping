{
    "name": "fourdotsix/tenancy-mapping",
    "description": "Tenant data handling for stancl/tenancy ",
    "keywords": [
        "Four Dot Six",
        "laravel",
        "tenancy-mapping"
    ],
    "homepage": "https://github.com/fourdotsix/tenancy-mapping",
    "license": "MIT",
    "authors": [
        {
            "name": "Hash",
            "email": "11258035+secrethash@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "dallgoot/yaml": "^1.0",
        "illuminate/contracts": "^11.0||^12.0",
        "spatie/laravel-package-tools": "^1.16",
        "stancl/tenancy": "dev-master"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Fourdotsix\\TenancyMapping\\": "src/",
            "Fourdotsix\\TenancyMapping\\Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/Helpers/mapping.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fourdotsix\\TenancyMapping\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fourdotsix\\TenancyMapping\\TenancyMappingServiceProvider"
            ],
            "aliases": {
                "TenancyMapping": "Fourdotsix\\TenancyMapping\\Facades\\TenancyMapping"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
